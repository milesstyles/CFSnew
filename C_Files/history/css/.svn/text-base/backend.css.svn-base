}
#header {
	width: 968px;
	border-bottom: 1px solid #4b4a4d;
	height: 105px;
	margin-bottom: 30px;
}
#header h1 {
	color: #FFFFFF;
	font-size: 2.5em;
	padding: 20px 0px 30px 26px;
}
#header h1 a {
	color: #fff;
	text-decoration: none;
}
#leftColumn {
	display: inline;
	float: left;
	width: 240px;
	margin-left: 15px;
	font-size: 1.1em;
}
#fullColumn td, #leftColumn td, #rightColumn td, .triCol td{
	padding: 4px 3px;
	vertical-align: middle;
}
#fullColumn th, #leftColumn th, #rightColumn th, .triCol th {
	padding: 4px 3px;
	font-weight: bold;
	text-align: left;
	color: #fff;
	border-bottom: 3px solid #1B1B1B;
}
#rightColumn {
	float: left;
	width: 683px;
	font-size: 1.1em;
	margin-left: 15px;
}
#sortableList { margin-top: 20px; }
#sortableList li {
	margin-bottom: 5px;
}
#sortableList li a {
	color: #060606;
	text-decoration: none;
	font-weight: bold;
	display: block;
	padding: 8px 4px;
	background-color: #CCCCCC;
}
#sortableList li a span {
	float: right;
	text-decoration: underline;
}


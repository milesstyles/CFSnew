}
#container {
	margin: 0px auto;
	width: 968px;
	text-align: left;
	padding: 0px 25px;
	background-image: url(../images/mainBGGlow.png);
	background-repeat: repeat-y;
}
#content {
	width: 968px;
	background-color: #08070b;
	min-height: 600px;
}
#content  h3 {
	color: #fff;
	font-size: 1.5em;
	font-weight: bold;
	padding: 0px 0px 15px 30px;
	display: block;
}

#fullColumn {
	width: 938px;
	margin-left: 15px;
	color: #A7A9Ac;
	font-size: 1.1em;
}
#footer {
	width: 968px;
	padding-top: 14px;
	padding-bottom: 14px;
	text-transform: uppercase;
	color: #A7A9AC;
	text-align: center;
}
#footer a {
	color: #A7A9AC;
